<?xml version="1.0" encoding="UTF-8"?>

<channel name="xfce4-screensaver" version="1.0">
  <property name="saver" type="empty">
    <property name="mode" type="int" value="2"/>
    <property name="idle-activation" type="empty">
      <property name="delay" type="int" value="30"/>
    </property>
    <property name="fullscreen-inhibit" type="bool" value="true"/>
    <property name="themes" type="empty">
      <property name="list" type="array">
        <value type="string" value="screensavers-xfce-floaters"/>
      </property>
    </property>
  </property>
  <property name="screensavers" type="empty">
    <property name="xfce-floaters" type="empty">
      <property name="number-of-images" type="int" value="60"/>
      <property name="arguments" type="string" value="-n 60"/>
      <property name="show-paths" type="bool" value="false"/>
      <property name="do-rotations" type="bool" value="true"/>
      <property name="print-stats" type="bool" value="false"/>
    </property>
    <property name="xfce-personal-slideshow" type="empty">
      <property name="location" type="string" value="/home/furdox/Downloads"/>
      <property name="arguments" type="string" value="--location=/home/furdox/Downloads --background-color=#000000"/>
    </property>
  </property>
  <property name="lock" type="empty">
    <property name="saver-activation" type="empty">
      <property name="delay" type="int" value="1"/>
    </property>
  </property>
</channel>
